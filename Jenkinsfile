pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Welcome') {
            steps {
                echo 'Welcome'
            }
        }
        stage('Hey') {
            steps {
                echo 'Hey you are a such a good person!'
            }
        }
        stage('ily') {
            steps {
                echo 'I Love You'
            }
        }
    }
}
